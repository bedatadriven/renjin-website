---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  <url>
    <loc>http://www.renjin.org/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>

  <url>
    <loc>http://www.renjin.org/documentation/index.html</loc>
    <changefreq>weekly</changefreq>
  </url>

  <url>
    <loc>http://www.renjin.org/documentation/contributor-guide.html</loc>
    <changefreq>weekly</changefreq>
  </url>

  <url>
    <loc>http://www.renjin.org/documentation/developer-guide.html</loc>
    <changefreq>weekly</changefreq>
  </url>

  <url>
    <loc>http://www.renjin.org/documentation/analyst-guide.html</loc>
    <changefreq>weekly</changefreq>
  </url>

  <url>
    <loc>http://www.renjin.org/blog/index.html</loc>
    <changefreq>weekly</changefreq>
  </url>

  {% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ post.url }}</loc> 
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>never</changefreq>
  </url>
  {% endfor %}

</urlset>
